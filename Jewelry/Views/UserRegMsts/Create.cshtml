@model Jewelry.Models.UserRegMst

@{
    ViewData["Title"] = "Create";
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Skydash Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="../../vendors/feather/feather.css">
    <link rel="stylesheet" href="../../vendors/ti-icons/css/themify-icons.css">
    <link rel="stylesheet" href="../../vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="../../css/vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="../../images/favicon.png" />
</head>

<body>
    <div class="container-scroller">
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="content-wrapper d-flex align-items-center auth px-0">
                <div class="row w-100 mx-0">
                    <div class="col-lg-4 mx-auto">
                        <div class="auth-form-light text-left py-5 px-4 px-sm-5">
                            <div class="brand-logo">
                                <img src="../../images/logo.svg" alt="logo">
                            </div>
                            <h4>New here?</h4>
                            <h6 class="font-weight-light">Signing up is easy. It only takes a few steps</h6>
                            <form class="pt-3" asp-action="Create">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="form-group">
                                    <label>First Name</label>
                                    <input asp-for="userFname" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Last Name</label>
                                    <input asp-for="userLname" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Address</label>
                                    <input asp-for="address" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>City</label>
                                    <input asp-for="city" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>State</label>
                                    <input asp-for="state" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Mobile Number</label>
                                    <input asp-for="mobNo" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <input asp-for="emailID" class="form-control" />
                                </div>
                                <div class="form-group">
                                    <label>Date of Birth</label>
                                    <input asp-for="dob" class="form-control" />
                                </div>
                               @* <div class="form-group">
                                    <label>Current date</label>
                                    <input asp-for="cdate" class="form-control" />
                                </div>*@
                                <div class="form-group">
                                    <label>Password</label>
                                    <input asp-for="password" class="form-control" />
                                </div>


                                <div class="form-group">
                                    <input class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn" type="submit" value="Sign Up">

                                </div>

                                <div class="text-center mt-4 font-weight-light">
                                    Already have an account? <a asp-controller="UserRegMsts" asp-action="userlogin" class="text-primary">Login</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- content-wrapper ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="../../vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="../../js/off-canvas.js"></script>
    <script src="../../js/hoverable-collapse.js"></script>
    <script src="../../js/template.js"></script>
    <script src="../../js/settings.js"></script>
    <script src="../../js/todolist.js"></script>
    <!-- endinject -->
</body>

</html>









